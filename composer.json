{
  "name": "strider2038/json-rpc-client",
  "description": "Flexible JSON RPC v2 client for PHP",
  "keywords": [
    "jsonrpc",
    "jsonrpc2",
    "client",
    "http",
    "tcp"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Igor Lazarev",
      "email": "strider2038@yandex.ru"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "phake/phake": "^3.1",
    "ramsey/uuid": "^3.8",
    "guzzlehttp/guzzle": "^6.3",
    "psr/log": "^1.1",
    "symplify/easy-coding-standard": "^7.0",
    "phpstan/phpstan": "^0.12.5"
  },
  "suggest": {
    "ramsey/uuid": "To generate UUID identifiers for request objects",
    "guzzlehttp/guzzle": "To use HTTP transport via Guzzle",
    "psr/log": "To use logging capabilities"
  },
  "autoload": {
    "psr-4": {
      "Strider2038\\JsonRpcClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Strider2038\\JsonRpcClient\\Tests\\": "tests/"
    }
  }
}
